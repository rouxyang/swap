html, body, div, span, object, h1, h2, h3, h4, h5, h6, p, a, em, img, dl, dt, dd,
ul, ol, li, form, fieldset, label, table, caption, tbody, tfoot, thead, tr, th,
td, pre, blockquote {
    margin: 0;
    padding: 0;
    vertical-align: baseline;
}
html {
    background: #fff;
}
body {
    position: relative;
    left: 0;
    top: 0;
    font-family: "微软雅黑", "宋体";
    font-size: 12px;
    letter-spacing: 0;
    word-spacing: 0;
    color: #000;
}
ul, ol {
    list-style-type: none;
}
a {
    text-decoration: none;
}
a:hover {
    text-decoration: underline;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
:focus {
    outline: 0;
}
.clearfix {
    clear: both;
}
